    "def rotateArray(nums,k):\n",
    "    n = len(nums)\n",
    "    print (n)\n",
    "    k = k % n\n",
    "    print (k)\n",
    "    print (nums[-k:])\n",
    "    print (nums[:-k])\n",
    "    nums[:] = nums[-k:] + nums[:-k]\n",
    "\n",
    "nums1 = [1, 2, 3, 4, 5, 6, 7]\n",
    "rotateArray(nums1, 3)\n",
    "\n"
   ]
